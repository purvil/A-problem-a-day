   "source": [
    "def kthGrammar(N: int, K: int) -> int:\n",
    "    if N == 1 and K == 1:\n",
    "        return 0\n",
    "    prev_digit = self.kthGrammar(N - 1, (K + 1) // 2)\n",
    "    if K % 2 == 0:\n",
    "        return 1 if prev_digit == 0 else 0\n",
    "    else:\n",
    "        return 0 if prev_digit == 0 else 1"
   ]
